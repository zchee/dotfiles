#!/usr/bin/env zsh

# -----------------------------------------------------------------------------
# directory hash

[[ -n "$ZSH_DEBUG" ]] && STARTTIME="$(($(gdate +%s%N)/1000000))"
# ----------------------------------------------------------------------------

# go
hash -d go="${prefix}/go"

# -----------------------------------------------------------------------------
[[ -n "$ZSH_DEBUG" ]] && ENDTIME="$(($(gdate +%s%N)/1000000))" && printf "loaded dirhash.zsh: %19s\\n" $(($ENDTIME - $STARTTIME))
